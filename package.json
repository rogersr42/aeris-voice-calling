    "openai": "^4.28.0",
    "pcm-convert": "^1.6.5",
    "twilio": "^5.0.0",
    "uuid": "^9.0.1",
    "wav": "^1.0.2",
    "wav-encoder": "^1.3.0",
    "ws": "^8.16.0"
